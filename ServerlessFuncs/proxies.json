{
  "$schema": "https://json.schemastore.org/proxies",
  "proxies": {
    "proxyHomePage": {
      "matchCondition": {
        "methods": ["GET"],
        "route": "/"
      },
      "backendUri": "https://storageaccountgbplm97dc.blob.core.windows.net/website/index.html"
    },
    "proxyContent": {
      "matchCondition": {
        "methods": ["GET"],
        "route": "/content/{*restOfPath}"
      },
      "backendUri": "https://storageaccountgbplm97dc.blob.core.windows.net/website/content/{restOfPath}"
    }
  }
}
